<%= render 'books/list', books: @books %>

 <div class="col-md-8 offset-md-1">
      <h2>Users</h2>
      <table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th></th>
      <th>Image</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <table>
<% @users.each do |user| %>
<div>
  <tr>
  <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: '/assets/images/no_image.jpg'%></td>
  <td><%= user.name %></td>
  <td><%= link_to "Show", user_path(user.id) %></td>
  </tr>
</div>
<% end %>
</table>
    </div>
  </div>
</div>